FROM 
	medico m, hojaresultados hr, paciente p, resultado r, analito a WHERE 
	m.MED_ID = hr.MED_ID AND p.PAC_ID = hr.PAC_ID AND hr.HR_ID = r.HR_ID AND r.ANA_ID = a.ANA_ID AND hr.HR_FechaEmision BETWEEN $P{FechaIni} AND $P{FechaFin}
